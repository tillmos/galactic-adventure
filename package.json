{
  "name": "galactic-spacefarers",
  "version": "1.0.0",
  "description": "Galactic Spacefarer Adventure CAP Project",
  "scripts": {
    "start": "cds run",
    "watch": "cds watch",
    "watch-project1": "cds watch --open galactic.project1/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "dependencies": {
    "@cap-js/sqlite": "^2.0.2",
    "@sap/cds": "^9.3.1",
    "dotenv": "^17.2.2",
    "nodemailer": "^7.0.6",
    "sqlite3": "^5"
  },
  "engines": {
    "node": ">=16"
  },
  "private": true,
  "cds": {
    "requires": {
      "db": {
        "kind": "sql",
        "model": [
          "db/schema.cds"
        ],
        "credentials": {
          "url": "sqlite:sqlite.db"
        }
      }
    }
  },
 "devDependencies": {
  "@sap/generator-fiori": "^1.18.7",
  "@sap/ux-specification": "^1.124.18",
  "@sap/ux-ui5-tooling": "1.16.3",
"cds-plugin-ui5": "^0.13.0"
},
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/project1"
  ]
}
